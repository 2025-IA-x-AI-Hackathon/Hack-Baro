  "main": "./.erb/dll/main.bundle.dev.js",
  "scripts": {
    "prebuild": "pnpm run i18n:generate-types",
    "i18n:generate-types": "pnpm -w --filter @baro/i18n-tools run types",
    "i18n:scan": "pnpm -w --filter @baro/i18n-tools run scan",
    "i18n:watch": "pnpm -w --filter @baro/i18n-tools run watch",
    "build": "concurrently \"pnpm run build:main\" \"pnpm run build:renderer\"",
    "build:dll": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=\"-r ts-node/register --no-warnings\" webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=\"-r ts-node/register --no-warnings\" webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=\"-r ts-node/register --no-warnings\" webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "postinstall": "node ./.erb/scripts/postinstall.js",
    "lint": "cross-env NODE_ENV=development eslint . --format=json --quiet | node -e \"const fs=require('fs');const raw=fs.readFileSync(0,'utf-8')||'[]';const startObj=raw.indexOf('[{');const startEmpty=raw.indexOf('[]');const sliceAt=startObj>=0?startObj:(startEmpty>=0?startEmpty:-1);const input=sliceAt>=0?raw.slice(sliceAt):'[]';let data=[];try{data=JSON.parse(input);}catch(e){console.error('Failed to parse ESLint JSON output');console.error(raw);process.exit(1);}const errors=data.reduce((s,f)=>s+f.errorCount,0);const warnings=data.reduce((s,f)=>s+f.warningCount,0);console.log(errors+' errors, '+warnings+' warnings');if(errors>0){process.exit(1);}\"",
    "lint:fix": "cross-env NODE_ENV=development eslint . --fix",
    "package": "ts-node ./.erb/scripts/clean.js dist && pnpm run build && electron-builder build --publish never && pnpm run build:dll",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "prestart": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=\"-r ts-node/register --no-warnings\" webpack --config ./.erb/configs/webpack.config.main.dev.ts",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && pnpm run prestart && pnpm run start:renderer",
    "start:main": "concurrently -k -P \"cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --watch --config ./.erb/configs/webpack.config.main.dev.ts\" \"electronmon . -- {@}\" --",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=\"-r ts-node/register --no-warnings\" webpack --config ./.erb/configs/webpack.config.preload.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true NODE_OPTIONS=\"-r ts-node/register --no-warnings\" webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "vitest run",
    "dev": "pnpm run start",
    "type-check": "tsc --project tsconfig.json --noEmit --pretty",
    "clean": "rimraf release",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test -c e2e/playwright.config.js"
  },
  "browserslist": [
    "extends browserslist-config-erb"
  ],
  "dependencies": {
    "@electron/notarize": "^3.1.0",
    "@heroui/react": "2.8.5",
    "@heroui/theme": "2.4.23",
    "@logtail/browser": "^0.5.6",
    "@logtail/node": "^0.5.6",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@mediapipe/pose": "^0.5.1675469404",
    "@mediapipe/tasks-vision": "^0.10.3",
    "@sentry/electron": "^7.1.1",
    "@sentry/node": "^10.19.0",
    "better-sqlite3": "^12.4.1",
    "chalk": "^4.1.2",
    "drizzle-orm": "^0.44.6",
    "electron-debug": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.24",
    "i18next": "^25.6.0",
